{
  "name": "config4live",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "lint": "standard --fix \"src/**/*.js\" \"test/**/*.js\" ",
    "preversion": "npm run test:all",
    "test": "jest",
    "test:all": "npm run lint && npm run test:typescript && npm run test",
    "test:coverage": "covert test/*.js"
  },
  "files": [
    "README.md",
    "src/"
  ],
  "license": "MIT",
  "devDependencies": {
    "@grpc/proto-loader": "^0.5.5",
    "codecov": "^3.8.0",
    "covert": "^1.1.1",
    "grpc": "^1.24.3",
    "jest": "^26.6.1",
    "logdown": "^3.3.1",
    "lru-cache": "^6.0.0",
    "node-cache": "^5.1.2",
    "standard": "^16.0.0"
  },
  "jest": {
    "roots": [
      "test/"
    ],
    "testRegex": "test/.*\\.js$",
    "collectCoverage": true,
    "coverageDirectory": "./coverage/"
  }
}
